function [output_args] = area_under_curve(data, target_number_of_features)

    X = data.X';
    number_features = size(X, 2);
    auc_scores = zeros(1, number_features);
    
    for i=1:number_features
        [,~,~,auc] = perfcurve(data.y, data.X(i,:), 1);
        auc_scores(i) = auc;
    end

    %FIXME: DO DID FOR ALL THE FEATURES

    [FPR,TPR,T,AUC] = perfcurve(data.y, data.X(10,:), 1);
    AUC
    figure;
    hold on;
    plot(FPR,TPR);

end